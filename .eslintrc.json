{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-return-await" : "off",
        "consistent-return" : "off",
        "no-throw-literal" : "off",
        "no-param-reassign" : "off",
        "no-console": "off"
    }
}
